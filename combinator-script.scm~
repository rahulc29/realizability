#lang racket
(define (lhs term)
  (list-ref term 0))
(define (rhs term)
  (list-ref term 1))
(define (lambda-star var term)
  (match term
    [sym #:when (eqv? sym var)
           '((S K) K)]
    [sym (quasiquote (K (unquote sym)))]
    [(list lhs rhs) (quasiquote ((S (unquote (lambda-star var lhs))) (unquote (lambda-star var rhs))))]))